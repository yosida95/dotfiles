{
  "clangd": {
    "cmd": ["clangd-8", "--background-index"]
  },
  "gopls": {
    "args": ["-remote=auto"],
    "initialization_options": {
      "analyses": {
        "nilness": true,
        "useany": true,
        "unusedparams": true,
        "unusedwrite": true
      },
      "buildFlags": [],
      "completeUnimported": true,
      "experimentalPostfixCompletions": false,
      "matcher": "Fuzzy",
      "staticcheck": false
    }
  },
  "sumneko-lua-language-server": {
    "cmd": ["lua-language-server"]
  },
  "yaml-language-server": {
    "workspace_config": {
      "yaml": {
        "schemaStore": {
          "enable": false
        }
      }
    }
  }
}
